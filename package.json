{
  "name": "driveoff",
  "version": "0.5.0.0",
  "repository": "git@github.com:clovfr/DriveOff.git",
  "author": "Clovis Gauzy <github.788acf@clovisgauzy.fr>",
  "private": true,
  "devDependencies": {
    "standard": "^16.0.3",
    "web-ext": "^5.5.0"
  },
  "scripts": {
    "start": "web-ext run --source-dir ./src/ --target=firefox-desktop --target=chromium --verbose",
    "start:firefox": "web-ext run --source-dir ./src/ --verbose",
    "start:chromium": "web-ext run --source-dir ./src/ --target=chromium --verbose",
    "build": "web-ext build --source-dir ./src/"
  },
  "webExt": {
    "build": {
      "overwriteDest": true
    },
    "artifactsDir": "./builds"
  } 
}
